{
  "name": "linter-styled-components",
  "main": "./lib/linter-styled-components",
  "version": "0.0.0",
  "description": "Atom Linter plugin for styled-components",
  "keywords": [],
  "repository": "https://github.com/nhajidin/linter-styled-components",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-linter": "^8.0.0",
    "atom-package-deps": "^4.3.1"
  },
  "devDependencies": {
    "eslint": "^3.8.0",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-plugin-import": "^2.0.1"
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "global-require": "off",
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "atom"
          ]
        }
      ]
    },
    "globals": {
      "atom": true
    },
    "env": {
      "es6": true,
      "node": true
    }
  }
}
